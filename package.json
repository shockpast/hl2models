		"@sveltejs/kit": "2.5.24",
		"@types/node": "^22.5.0",
		"@types/three": "0.167.2",
		"autoprefixer": "^10.4.20",
		"flowbite": "^2.5.1",
		"flowbite-svelte": "^0.46.15",
		"flowbite-svelte-icons": "^1.6.1",
